<meta charset="UTF-8" >
<meta name="viewport" content="user-scalable=no,width=device-width,initial-scale=1,maximum-scale=1">        
{% block head_title %}
    {% set title=get_meta('title') %}
    <title>{{ brand_title()|default('Antares') }}  {{ title|length > 0 ? '- '~title:'' }}</title>
{% endblock %}
<meta name="theme-color" content="#02A8F3">
<meta name="description" content="Thank you for choosing Antares as a foundation for your application. You can find the latest updates and documentation at: http://antaresproject.io" >
<meta name="keywords" content="Antares, User Management, Platform, Laravel, Two-Factor Authentication, REST API,Notifications, Automation, Open Source, Installer, Updater" >
<meta name="author" content="INBS Software" >
<meta NAME="robots" content="noindex, nofollow">
<link rel="icon" sizes="192x192" href="{{ brand_logo('favicon') }}">
<link rel="icon" href="{{ brand_logo('favicon') }}">
<meta name="msapplication-navbutton-color" content="#02A8F3">
<meta name="apple-mobile-web-app-status-bar-style" content="#02A8F3">
<meta name="application-name" content="Antares">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="apple-mobile-web-app-title" content="Antares">        
{% block javascripts %}
    {%  set asset = app('antares.asset').container('antares.header').style('external_css', '/css/external.css').style('antares_css', '/css/antares.css') %} 
    {{ asset.styles()|raw }}
{% endblock %}
{{ brand_styles()|raw }}