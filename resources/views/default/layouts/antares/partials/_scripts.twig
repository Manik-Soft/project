{% block javascripts %}
    {%  set asset = app('antares.asset').container('antares/foundation::footer')
               .script('antares_assets',    '/_dist/js/external/antares_assets.js')
               .script('mechanics',    '/_dist/js/mechanics.js',['bower_assets'])   
               .script('notifications',    '/_dist/js/notifications.js',['mechanics'])
               .script('user-activity',    'packages/core/js/user-activity.js',['mechanics'])
               .script('dropdowns-js', '_dist/js/dropdowns.js',['notifications'])                                        
               .script('menu-js',    '/_dist/js/menu.js',['dropdowns-js'])                                        
               .script('forms',    '/_dist/js/forms.js',['menu-js']) 
               .script('filters-js', '_dist/js/components/filters.js',['forms'])
               .script('tinycon-js', 'packages/core/js/tinycon.min.js',['filters-js'])
               .script('on-load', 'packages/core/js/on-load.js',['tinycon-js'])                                                      
               .script('datatable-helpers-js', 'packages/core/js/datatable-helpers.js',['on-load'])               
    %}     
    {{ asset.styles()|raw }} 
    {{ asset.scripts()|raw }}
{% endblock %}
{{ placeholder("antares.layout: footer") }}
{{ stack('antares.footer') }}
{% 
    set asset = app('antares.asset').container('antares/foundation::scripts')
%}
{{ asset.styles()|raw }}
{{ asset.scripts()|raw }}

{{ placeholder("antares.layout: scripts") }}
{{ stack('antares.scripts') }}
{% if extension_active('multibrand') %}
    {{ brandstyles|raw }}
{% endif %}
